<%-include("partials/header")%>
<link rel="stylesheet" href="css/display.css">
<div class="container search">
<form class="form-inline my-2 my-lg-0" action="/patientDisplay" method="post">
     <input class="form-control mr-sm-2" type="search" placeholder="Search by Patient_id/Name">
     <button class="btn btn-outline-success" type="submit">Search</button>
   </form>
 </div>
<div class="container-fluid tableContent">
<table class="table table-bordered">
<thead class="thead-dark">
  <tr>
    <th scope="col">Patient_id</th>
    <th scope="col">Name</th>
    <th scope="col">Mobile No.</th>
    <th scope="col">Address</th>
    <th scope="col">Country</th>
    <th scope="col">State</th>
    <th scope="col">City</th>
    <th scope="col">Zip code</th>
    <th scope="col">Gender</th>
    <th scope="col">DOB</th>
    <th scope="col">Blood Grp</th>
    <th scope="col">Insurance No.</th>
    <th scope="col">Insurance Expiry Date</th>
    <th scope="col">Marital Status</th>
    <th scope="col">Delete</th>
  </tr>
</thead>
<tbody>

  <% for(let key in display){%>
    <tr>
      <td><%=display[key].patient_id%></td>
      <td><%=display[key].patient_name%></td>
      <td><%=display[key].patient_mobile%></td>
      <td><%=display[key].patient_address%></td>
      <td><%=display[key].patient_country%></td>
      <td><%=display[key].patient_state%></td>
      <td><%=display[key].patient_city%></td>
      <td><%=display[key].zipcode%></td>
      <td><%=display[key].gender%></td>
      <td><%=display[key].patient_DOB%></td>
      <td><%=display[key].patient_blood%></td>
      <td><%=display[key].patient_insurance%></td>
      <td><%=display[key].patient_insexp%></td>
      <td><%=display[key].patient_mstatus%></td>
      <td><button type="button" name="button" class="btn btn-sm" id="'<%=display[key].patient_id%>'"><i class="fas fa-trash-alt"></i></button></td>
    </tr>
<%}%>
</tbody>
</table>
</div>
<%-include("partials/footer")%>
